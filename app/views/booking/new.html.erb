<h1>New reservation</h1>
<%= form_for :booking, url:{controller: 'booking', action: 'create'} do |f| %>
  <p>
    <%= f.label :Check_in_date %>
    <%= f.datetime_field :check_in_date%>

    <%= f.label :to%>
    <%= f.datetime_field :check_out_date%>
  </p>

  <p>
    <%= f.label :number_of_children%>
    <%= f.number_field :number_of_children%>
  </p>
  <p>
    <%= f.label :number_of_guest%>
    <%= f.number_field :number_of_guest%>
  </p>
  <dp>
    <%= f.submit %>
  </p>
<% end %>